M = int(input('Quantas linhas vai ter a matriz? \n'))
N = int(input('Quantas colunas vai ter a matriz? \n\n'))

mat = [[0 for x in range(N)] for x in range(M)]

for i in range(0, M):
    for j in range(0,N):
        mat[i][j] = int(input(f'Elemento [{i},{j}]: \n\n'))
        
print('MATRIZ DIGITADA:')
for i in range (0, M):
    for j in range(0, N):
        print(f'{mat[i][j]} ', end='')
    print()
